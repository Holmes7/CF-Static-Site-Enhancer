{
	"name": "cf-nos",
	"version": "1.0",
	"description": "Shows the number of submissions on the static (m1,m2,m3) codeforces sites",
	"manifest_version": 2,
	"content_scripts": [
		{
			"matches": [
				"*://m1.codeforces.com/contest/*",
				"*://m2.codeforces.com/contest/*",
				"*://m3.codeforces.com/contest/*"
			],
			"js": [
				"main.js"
			]
		}
	],
	"background": {
		"scripts": [
			"background.js"
		],
		"persistent": false
	},
	"permissions": [
		"https://codeforces.com/*",
		"tabs"
	]
}